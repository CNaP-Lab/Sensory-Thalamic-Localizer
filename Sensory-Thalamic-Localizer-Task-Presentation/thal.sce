bitmap { filename = "checkerboardInv.tiff"; width = 1080; height = 1080; transparent_color = 149,149,149; }check2_map;

picture { bitmap check1_map; x = 0; y = 0 ; bitmap mouse_fix; x = 0; y = 0;}check1;
