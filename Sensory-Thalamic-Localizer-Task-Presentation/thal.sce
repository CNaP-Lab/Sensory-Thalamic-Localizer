#box { height = 20; width = 20; color = 67,67,67;}black;
box { height = 20; width = 20; color = 0,0,0;}black;

#box { height = 20; width = 20; color = 187,187,187;}white;
box { height = 20; width = 20; color = 255,255,255;}white;

text { caption = "Waiting for scanner..."; }wait;
text { caption = "Waiting for scan prep..."; }prep;
picture { text prep; x = 0; y = 0;}ins_pic;


