scenario = "Thalamic_Localizer";
active_buttons = 2;     
button_codes = 100,200;
default_background_color=127,127,127;
default_text_color=255,255,255;
default_font_size = 36;

scan_period=800;
pulses_per_scan=1;
pulse_code=1;
###########################

scenario_type = fMRI;
#scenario_type = fMRI_emulation;

###########################

pcl_file="thal.pcl";

begin;

bitmap { filename = "fixation.bmp"; width = 200; height = 200; transparent_color = 0,0,0; }mouse_fix;

picture {bitmap mouse_fix; x = 0; y = 0;}default;

